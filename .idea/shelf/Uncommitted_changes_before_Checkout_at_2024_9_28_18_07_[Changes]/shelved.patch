+# 创建生成器
+
+# 本质是对象，
+my_generator = (i * 2 for i in range(5))
+print(my_generator)
+
+value = next(my_generator)
+print(value)
+
+for value in my_generator:
+    print(value)
+
+
+def generator(n):
+    for i in range(n):
+        print("生成器正在运行")
+        yield i
+        print("生成器运行结束")
+
+
+g = generator(5)
+# print(next(g))
+# print(next(g))
+# print(next(g))
+# print(next(g))
+# print(next(g))
+#
+# print(next(g))
+# print(next(g))
+
+for i in g:
+    print(i)
+
+
+def fib(max):
+    n, a, b = 1, 1, 1,
+    while n <= max:
+        yield a
+        a, b = b, a + b
+        n += 1
+
+
+for i in fib(10):
+    print(i)
+
